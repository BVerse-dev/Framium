{
  "functions": {
    "api/stripe/webhook.ts": {
      "memory": 512,
      "maxDuration": 10
    },
    "api/chat.ts": {
      "memory": 1024,
      "maxDuration": 15
    },
    "api/auth/login.ts": {
      "memory": 256,
      "maxDuration": 5
    },
    "api/auth/signup.ts": {
      "memory": 256,
      "maxDuration": 5
    },
    "api/user/preferences.ts": {
      "memory": 256,
      "maxDuration": 5
    },
    "api/tasks/create.ts": {
      "memory": 512,
      "maxDuration": 10
    }
  },
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" },
        { "key": "Access-Control-Allow-Methods", "value": "GET, POST, PUT, DELETE, OPTIONS" },
        { "key": "Access-Control-Allow-Headers", "value": "Content-Type, Authorization" }
      ]
    }
  ]
}
