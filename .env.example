# Environment Variables for Framium Backend
# Copy to .env.local and fill in your actual values

# Database
DATABASE_URL=postgres://username:password@your-vercel-postgres-url/framium

# Stripe Configuration
STRIPE_SECRET_KEY=sk_test_your_stripe_secret_key
STRIPE_WEBHOOK_SECRET=whsec_your_webhook_secret
STRIPE_PUBLISHABLE_KEY=pk_test_your_publishable_key

# AI Model API Keys
OPENAI_API_KEY=sk-your-openai-api-key
ANTHROPIC_API_KEY=sk-ant-your-anthropic-api-key
GEMINI_API_KEY=your-google-gemini-api-key

# Authentication
JWT_SECRET=your-super-secret-jwt-signing-key
NEXTAUTH_SECRET=your-nextauth-secret-for-production
NEXTAUTH_URL=https://your-domain.vercel.app

# Optional: Advanced Configuration
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX_REQUESTS=100
LOG_LEVEL=info

# Development Only
NODE_ENV=development
